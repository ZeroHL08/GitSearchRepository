  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/docs/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
